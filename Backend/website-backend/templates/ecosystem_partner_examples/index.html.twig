{% extends 'base.html.twig' %}

{% block title %}EcosystemPartnerExamples index{% endblock %}

{% block body %}
    <h1>EcosystemPartnerExamples index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Example</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ecosystem_partner_example in ecosystem_partner_examples %}
            <tr>
                <td>{{ ecosystem_partner_example.id }}</td>
                <td>{{ ecosystem_partner_example.example }}</td>
                <td>
                    <a href="{{ path('app_ecosystem_partner_examples_show', {'id': ecosystem_partner_example.id}) }}">show</a>
                    <a href="{{ path('app_ecosystem_partner_examples_edit', {'id': ecosystem_partner_example.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_ecosystem_partner_examples_new') }}">Create new</a>
{% endblock %}
