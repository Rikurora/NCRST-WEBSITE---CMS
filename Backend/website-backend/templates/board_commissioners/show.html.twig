{% block body %}
    <h1>BoardCommissioners</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ board_commissioner.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ board_commissioner.name }}</td>
