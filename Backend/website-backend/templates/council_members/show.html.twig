    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ council_member.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ council_member.name }}</td>
            </tr>
            <tr>
