{% extends 'base.html.twig' %}

{% block title %}ResearchPriorities index{% endblock %}

{% block body %}
    <h1>ResearchPriorities index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Priority</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for research_priority in research_priorities %}
            <tr>
                <td>{{ research_priority.id }}</td>
                <td>{{ research_priority.priority }}</td>
                <td>
                    <a href="{{ path('app_research_priorities_show', {'id': research_priority.id}) }}">show</a>
                    <a href="{{ path('app_research_priorities_edit', {'id': research_priority.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_research_priorities_new') }}">Create new</a>
{% endblock %}
