{% extends 'base.html.twig' %}

{% block title %}IksKnowledgeAreaExamples index{% endblock %}

{% block body %}
    <h1>IksKnowledgeAreaExamples index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Example</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for iks_knowledge_area_example in iks_knowledge_area_examples %}
            <tr>
                <td>{{ iks_knowledge_area_example.id }}</td>
                <td>{{ iks_knowledge_area_example.example }}</td>
                <td>
                    <a href="{{ path('app_iks_knowledge_area_examples_show', {'id': iks_knowledge_area_example.id}) }}">show</a>
                    <a href="{{ path('app_iks_knowledge_area_examples_edit', {'id': iks_knowledge_area_example.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_iks_knowledge_area_examples_new') }}">Create new</a>
{% endblock %}
