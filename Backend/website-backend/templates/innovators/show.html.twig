{% extends 'base.html.twig' %}

{% block title %}Innovators{% endblock %}

{% block body %}
    <h1>Innovators</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ innovator.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ innovator.name }}</td>
            </tr>
            <tr>
                <th>Company</th>
                <td>{{ innovator.company }}</td>
            </tr>
            <tr>
                <th>Sector</th>
                <td>{{ innovator.sector }}</td>
            </tr>
            <tr>
                <th>Innovation</th>
                <td>{{ innovator.innovation }}</td>
            </tr>
            <tr>
                <th>Impact</th>
                <td>{{ innovator.impact }}</td>
            </tr>
            <tr>
                <th>Funding</th>
                <td>{{ innovator.funding }}</td>
            </tr>
            <tr>
                <th>Image_url</th>
                <td>{{ innovator.imageUrl }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ innovator.createdAt ? innovator.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_innovators_index') }}">back to list</a>

    <a href="{{ path('app_innovators_edit', {'id': innovator.id}) }}">edit</a>

    {{ include('innovators/_delete_form.html.twig') }}
{% endblock %}
